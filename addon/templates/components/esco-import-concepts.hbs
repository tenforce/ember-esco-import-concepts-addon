
<form id="fileForm">
  <label>{{startingMessage}}</label>
  {{input type="file" name="fileToUpload" value=fileToUpload}}
  <button type="submit" disabled={{shouldDisable}}>Import</button>
</form>

{{#if isFileLoaded}}
  {{#radio-button value="utf-8" groupValue=encoding changed="radioButtonSelected"}}
    TSV
  {{/radio-button}}

  {{#radio-button value="utf-16" groupValue=encoding changed="radioButtonSelected"}}
    Unicode Text
  {{/radio-button}}
{{/if}}



{{#if showError}}
  <div class="import-concepts-error">
    <p>{{errorTitle}}</p>
    <p>{{errorDetail}}</p>
  </div>
{{else}}
  <div class="import-concepts-status">
    <p>{{importStatus}}</p>
  </div>
{{/if}}
